@model IEnumerable<Dominio.Model.Atividade>


<h2>Atividades cadastradas</h2>

<div class="container">
    <h2>Atividades entregues</h2>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Nome</th>
                <th>URL</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Nome</td>
                    <td>@item.UrlGitHub</td>
                </tr>
            }
        </tbody>
    </table>
</div>
